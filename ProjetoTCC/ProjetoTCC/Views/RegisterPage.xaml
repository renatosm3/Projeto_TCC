<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjetoTCC.Views.RegisterPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Quem é você?" />
            <Entry Placeholder="Nome" />
            <Entry Placeholder="Sobrenome" />
            <Label Text="Data Nascimento" />
            <DatePicker MinimumDate="01/01/1900" MaximumDate="12/31/2017"/>
            <Entry Placeholder="Email" Keyboard="Email"/>
            <Entry IsPassword="True" Placeholder="Senha"/>
            <Button Text="Confirmar" Command="{Binding ConfirmCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>